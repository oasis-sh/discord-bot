// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Tag {
  name      String   @id
  createdAt DateTime @default(now())
  content   String
  ownerID   String
  ownerName String
}

model ReactionRole {
  messageID String @id
  emojiID   String
  roleID    String
}
